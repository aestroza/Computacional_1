#include <stdio.h>

int main(int argc, char *argv[])
{
    int valor=2;
    int i=0,j=0;
    int array[2][2]={{1,2},{3,4}};

    printf("la variale tiene valor: %i\n", valor);

    for (i = 0; i < 2; ++i){
        for(j = 0; j < 2; ++j){
            printf("%i ", array[i][j]);
        }
        printf("\n");
    }

    return 0;   
}
